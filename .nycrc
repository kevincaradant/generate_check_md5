{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "tests/**/*",
    "bin/**/*",
    "coverage/**/*",
    "node_modules/**/*"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "require": [
      "ts-node/register"
   ],
  "all": true,
  "check-coverage": true
}

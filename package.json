{
  "name": "generate_check_md5",
  "version": "1.2.5",
  "description": "Generate and compare MD5 files",
  "main": "./bin/index.js",
  "scripts": {
    "start": "npm run build",
    "clean": "rimraf bin && rimraf coverage",
    "lint": "eslint src tests",
    "check": "npm run lint -s && dependency-check ./package.json",
    "watch": "watch 'npm run build' src tests",
    "pretest": "babel --plugins transform-remove-console --optional runtime src tests -d tmpTests",
    "test": "tape -r babel-register tmpTests/**/*.js | tspec",
    "build": "babel --optional runtime src -d bin",
    "postbuild": "npm run check -s && npm run test -s",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report lcovonly --report html tmpTests",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "autocl": "auto-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevincaradant/generate_check_md5.git"
  },
  "bin": {
    "gcmd5": "./bin/index.js"
  },
  "preferGlobal": true,
  "keywords": [
    "generate",
    "create",
    "generator",
    "comparator",
    "md5",
    "analyze md5",
    "compare md5"
  ],
  "author": "KÃ©vin CARADANT <kevin.caradant@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevincaradant/generate_check_md5/issues"
  },
  "homepage": "https://github.com/kevincaradant/generate_check_md5#readme",
  "dependencies": {
		"babel-core": "6.21.0",
		"babel-polyfill": "6.20.0",
    "cli-color": "1.1.0",
    "line-by-line": "0.1.5",
    "md5-file": "3.1.1",
    "recursive-readdir-sync": "1.0.6",
    "tap-spec": "4.1.1",
    "yargs": "6.5.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-remove-console": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "coveralls": "2.11.15",
    "dependency-check": "2.6.1",
    "eslint": "3.12.2",
    "eslint-config-xo-space": "0.15.0",
    "isparta": "4.0.0",
    "rimraf": "2.5.4",
    "tape": "4.6.3",
    "watch": "1.0.1"
  }
}
